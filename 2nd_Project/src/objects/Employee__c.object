    <validationRules>
        <fullName>DOB_of_Employee</fullName>
        <active>false</active>
        <errorConditionFormula>IF(  Age__c  &lt;18, true, false)</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_On_Notice_Period</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (Is_on_notice_period__c =false &amp;&amp; Resignation_Date__c  &lt;&gt; null) , true, false)</errorConditionFormula>
        <errorMessage>Resignation Date cannot be null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Is_on_notice_period__c  = false, true, false)</errorConditionFormula>
        <errorMessage>Resignation date cannot insert, if employee is not on notice period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_number_size</fullName>
        <active>true</active>
        <errorConditionFormula>IF( (LEN( TEXT( SSN__c ))&gt;10) , true, false)</errorConditionFormula>
        <errorMessage>SSN should be 10-digit number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_age_of_employee</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ((Age__c&lt;= 21) &amp;&amp;  (Age__c  &gt;=30)) , true, false)</errorConditionFormula>
        <errorMessage>Age of Employee should be between 21-30.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Working_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Last_Working_Day__c  &lt;  Date_of_Joining__c ,true,false)</errorConditionFormula>
        <errorMessage>Last working date should be greater than joining date.</errorMessage>
    </validationRules>
</CustomObject>
